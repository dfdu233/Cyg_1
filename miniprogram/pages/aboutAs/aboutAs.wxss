.logo {
  padding: 10rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.logo image {
  width: 100%;
  height: 350rpx;
  margin-top: 5rpx;
  border-radius: 10rpx;
}

.desc {
  background-color: #fff;
  padding: 30rpx;
  margin-top: 10rpx;
  font-size: 28rpx;
  border-radius: 25rpx;
}

.text {
  color: #a1a1a1;
  margin: 30rpx;
}

.cell {
  margin: 0 auto;
  background-color: #fff;
  padding: 30rpx;
  margin-bottom: 5rpx;
  border-radius: 25rpx;
  margin-top: 10rpx;
  width: 90% !important;
}