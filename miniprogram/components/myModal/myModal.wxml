<!--
封装的小提示的组件
我的-申请接单-点击申请接单常见问题说明
component的父组件：pages/applyOrder/applyOrder.json
-->

<view class="modal">
    <view class="top">
        <text>{{ title }}</text>
        <text bindtap="cancel">x</text>
    </view>
    <view class="content">
        {{ content }}
    </view>
    <view class="btn">
        <button type="primary" bindtap="submit">确定</button>
        <button bindtap="cancel">取消</button>
    </view>
</view>
